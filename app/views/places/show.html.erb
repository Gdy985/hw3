<h1><%= @place["name"] %></h1>

<ul>
  <% for post in @posts %>
    <h2>
    <%= post["title"] %>
    <h2>
    <li>
    <%= post["posted_on"] %>
    </li>
    <li>
    <%= post["description"] %>,
    </li>
  <% end %>
</ul>

<p>
  <a href="/posts/new?place_id=<%= @place["id"] %>">New Post</a>
</p>

<p>
<a href="/places">Back to All Places</a>
</p>

<p>
<%= button_to "Delete This Place", @place, :method => "delete" %>
</p>